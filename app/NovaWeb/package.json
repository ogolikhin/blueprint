{
  "name": "nova",
  "version": "2.0.1-0",
  "description": "Requirements Management Software Web Application",
  "private": true,
  "license": "UNLICENSED",
  "main": "/",
  "scripts": {
    "build": "webpack --config webpack/webpack.build.js --NODE_ENV=production",
    "start": "node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.js --content-base src --inline --hot --NODE_ENV=dev",
    "preinstall": "npm prune",
    "postinstall": "typings prune && typings install",
    "dev": "node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.js --content-base src --inline --hot --NODE_ENV=dev",
    "dev:open-browser": "npm config set nova_open_browser 1 && npm run dev",
    "dev:no-browser": "npm config set nova_open_browser 0 && npm run dev",
    "dev:build": "node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.js --content-base src --inline --hot --debug --NODE_ENV=dev ",
    "dev:public": "node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.js --content-base src --inline --hot --public --NODE_ENV=dev",
    "pretest": "node node_modules/tslint/bin/tslint -c tslint.json --project tsconfig.json -e src/**/*.spec.ts src/**/*.ts",
    "test": "karma start --NODE_ENV=test",
    "test:debug": "karma start karma.conf.js --browsers=Chrome --no-single-run --debug --NODE_ENV=test",
    "test:spec": "karma start karma.debug.js --browsers=Chrome --no-single-run --debug --NODE_ENV=test"
  },
  "dependencies": {
    "ag-grid": "~7.1.0",
    "ag-grid-enterprise": "~7.1.0",
    "angular": "~1.5.10",
    "angular-animate": "~1.5.10",
    "angular-formly": "^8.2.1",
    "angular-formly-templates-bootstrap": "^6.4.0",
    "angular-google-analytics": "^1.1.8",
    "angular-messages": "~1.5.10",
    "angular-mocks": "~1.5.10",
    "angular-module-mocks": "~1.2.19",
    "angular-sanitize": "~1.5.10",
    "angular-ui-bootstrap": "2.3.0",
    "angular-ui-router": "~0.2.17",
    "angular-ui-tinymce": "^0.0.17",
    "angular-vs-repeat": "^1.1.7",
    "api-check": "^7.5.5",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "bowser": "^1.5.0",
    "custom-event-polyfill": "^0.3.0",
    "es6-shim": "^0.35.1",
    "jquery": "^3.1.0",
    "keen-js": "^3.4.1",
    "lodash": "^4.16.4",
    "moment": "^2.15",
    "rx": "^4.1.0",
    "tinymce": "4.5.1",
    "ui-select": "0.19.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "^2.2.4",
    "command-line-args": "^3.0.3",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.2",
    "gulp-sass": "^3.0.0",
    "html-webpack-plugin": "^2.10.0",
    "http-proxy-middleware": "^0.17.2",
    "imports-loader": "^0.7.0",
    "istanbul": "^0.4.2",
    "istanbul-instrumenter-loader": "~0.2.0",
    "jasmine": "~2.5.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-es6-shim": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "~0.0.23",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^4.0.0",
    "path": "^0.12.7",
    "phantomjs-polyfill": "~0.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.1.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "proxy-middleware": "^0.15.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "sc5-styleguide": "^1.3.2",
    "script-loader": "~0.7.0",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.0",
    "tslint-eslint-rules": "3.3.0",
    "tslint-loader": "^2.1.3",
    "typescript": "~2.0.3",
    "typings": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.16.2",
    "webpack-fail-plugin": "^1.0.5"
  },
  "config": {
    "backend": "http://localhost:9801"
  },
  "engines": {
    "npm": "~3.10.0",
    "node": "~6.9.0"
  }
}
