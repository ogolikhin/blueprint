{
  "name": "nova",
  "version": "7.4.0-0",
  "description": "Requirements Management Software Web Application",
  "private": true,
  "license": "UNLICENSED",
  "main": "/",
  "scripts": {
    "preinstall": "npm prune && typings prune",
    "postinstall": "typings install",
    "dev": "node_modules/.bin/webpack-dev-server --progress --config webpack/webpack.dev.js --content-base src --inline --hot --NODE_ENV=dev",
    "dev:open-browser": "npm config set nova_open_browser 1 && npm run dev",
    "dev:no-browser": "npm config set nova_open_browser 0 && npm run dev",
    "dev:nolint": "npm run dev",
    "dev:build": "node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.js --content-base src --inline --hot --debug --NODE_ENV=dev ",
    "dev:public": "node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.js --content-base src --inline --hot --public --NODE_ENV=dev",
    "test": "karma start --NODE_ENV=test",
    "test:nolint": "karma start --debug --NODE_ENV=test",
    "test:debug": "karma start karma.conf.js --browsers=Chrome --no-single-run --debug --NODE_ENV=test",
    "test:spec": "karma start karma.debug.js --browsers=Chrome --no-single-run --debug --NODE_ENV=test",
    "build": "webpack --config webpack/webpack.build.js --NODE_ENV=production",
    "lint": "npm run lint:all",
    "lint:all": "tslint \"src/modules/**/*.ts\"",
    "lint:nospec": "tslint -e \"**/*.spec.ts\" -e \"**/*.mock.ts\" \"src/modules/**/*.ts\""
  },
  "dependencies": {
    "ag-grid": "6.2.0",
    "ag-grid-enterprise": "6.2.0",
    "angular": "~1.5.7",
    "angular-formly": "^8.2.1",
    "angular-formly-templates-bootstrap": "^6.4.0",
    "angular-messages": "~1.5.7",
    "angular-sanitize": "~1.5.2",
    "angular-ui-bootstrap": "^2.1.4",
    "angular-ui-router": "~0.2.17",
    "angular-ui-tinymce": "^0.0.17",
    "api-check": "^7.5.5",
    "bootstrap": "~3.3.6",
    "bowser": "~1.3.0",
    "lodash": "^4.16.4",
    "moment": "2.14.1",
    "ng-draggable": "~1.0.0",
    "path": "^0.12.7",
    "rx": "^4.1.0",
    "tinymce": "^4.4.3",
    "ui-select": "^0.19.1",
    "webpack-dev-server": "^1.16.2"
  },
  "devDependencies": {
    "angular-mocks": "~1.5.2",
    "angular-module-mocks": "~1.2.19",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.7.2",
    "babel-istanbul": "^0.11.0",
    "browser-sync": "^2.11.1",
    "browser-sync-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "^0.3.0",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.25.0",
    "del": "^2.2.0",
    "es6-shim": "^0.35.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-help": "^1.6.1",
    "gulp-npm-run": "^0.0.3",
    "html-webpack-plugin": "^2.10.0",
    "imports-loader": "^0.6.5",
    "istanbul": "^0.4.2",
    "istanbul-instrumenter-loader": "~0.2.0",
    "jasmine": "~2.5.0",
    "jquery": "^3.1.0",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-es6-shim": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "~0.0.23",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs-polyfill": "~0.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "proxy-middleware": "^0.15.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.0",
    "script-loader": "0.7.0",
    "style-loader": "^0.13.0",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3",
    "typescript": "~2.0.3",
    "typings": "^1.4.0",
    "url-loader": "~0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.16.2",
    "webpack-fail-plugin": "^1.0.5"
  },
  "config": {
    "backend": "http://localhost:9801"
  }
}
