<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <connectionStrings>
    <add name="TenantsDatabase" providerName="System.Data.SqlClient" connectionString="Data Source=BlueprintDevDB;Initial Catalog=Raptor;Integrated Security=True;Max Pool Size=80" />
  </connectionStrings>
  <log4net>
    <root>
      <!-- Valid values are All < Debug < Info < Warn < Error < Fatal < Off -->
      <level value="Warn" />
      <appender-ref ref="ServerFileAppender" />
    </root>
    <appender name="ServerFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="Log\BlueprintMessagingServices.log" />
      <appendToFile value="true" />
      <!-- Use a minimal locking model that allows multiple processes to write to the same file -->
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <!-- Max number of log files to keep. If not specified, default is 2 -->
      <maxSizeRollBackups value="5" />
      <!-- Default rolling style is by size. -->
      <!-- To enable rolling by date, set this value to "Date". (Not recommended as the number of files generated cannot be limited)-->
      <!-- To enable rolling by both date and size, set this value to "Composite". (Not recommended as the number of files generated cannot be limited)-->
      <rollingStyle value="Size" />
      <!-- Max size of the single log file in MB. -->
      <maximumFileSize value="10MB" />
      <!-- The newest log by size would have the highest suffix -->
      <countDirection value="1" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%property{logId}: %date{dd/MM/yyyy HH:mm:ss}, %property{timeZoneOffset}, ThreadId:%t, %property{sessionId}, %property{userName}, %property{level},%message%newline" />
      </layout>
    </appender>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <!--Connection string for SQL Server transport-->
    <add key="NServiceBus.ConnectionString" value="Data Source=BlueprintDevDB;Initial Catalog=Raptor;Integrated Security=True;Max Pool Size=80" />

    <!-- Specifies the list of supported action types. Default value is "All". We can also specify multiple types e.g., Property,Notification -->
    <!-- The list should be in a comma separated format. -->
    <!-- Supported Values -->
    <!-- Property -->
    <!-- Notification -->
    <!-- GenerateDescendants -->
    <!-- GenerateTests -->
    <!-- GenerateUserStories -->
    <!-- StateChange -->
    <!-- ArtifactsPublished -->
    <!-- Webhooks -->
    <!-- All -->
    <add key="SupportedActionTypes" value="All" />

    <!-- Webhook Configuration Settings -->
    <!-- Default Webhook Connection Timeout is 20 seconds -->
    <add key="WebhookConnectionTimeout" value="20" />
    <!-- Number of delayed retries - default is 5-->
    <add key="WebhookRetryCount" value="5" />
    <!-- Delayed retry timeout - default is 900 seconds (15 minutes) -->
    <add key="WebhookRetryInterval" value="900" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Http" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.3.0" newVersion="5.2.3.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Net.Http.Formatting" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.3.0" newVersion="5.2.3.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="RabbitMQ.Client" publicKeyToken="89e7d7c5feba84ce" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.0.0.0" newVersion="5.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
