<configuration>
	<appSettings>
		<add key="FileChunkSize" value="1"/>
	</appSettings>
	<connectionStrings>
    <add name="FileStoreDatabase"
      providerName="System.Data.SqlClient"
      connectionString="data source=.\SQLEXPRESS;initial catalog=Raptor_FileStorage;integrated security=True;min pool size=4;max pool size=128;load balance timeout=2;packet size=16384;connectretrycount=3;connectretryinterval=3;MultipleActiveResultSets=True;" />
    <add name="FileStreamDatabase"
        providerName="System.Data.SqlClient"
        connectionString="data source=.\SQLEXPRESS;initial catalog=Raptor;integrated security=True;min pool size=4;max pool size=128;load balance timeout=2;packet size=16384;connectretrycount=3;connectretryinterval=3;MultipleActiveResultSets=True;" />

    <!-- This should be removed once the file store is auto deployable. Before release of Dylan. -->
    <!--<add name="FileStreamDatabase"
        providerName="System.Data.SqlClient"
        connectionString="Data Source=SVMINTEGRATION;Initial Catalog=Integration_Trunk_Search;Persist Security Info=True;User ID=IntegrationTestUser;Password=$Blueprint99" />-->

  </connectionStrings>
	<system.web>
    <httpModules>
      <remove name="DomainServiceModule" />
    </httpModules>    
    <compilation debug="true" targetFramework="4.5.1"/>
		<httpRuntime targetFramework="4.5.1" enableVersionHeader="false" maxRequestLength="2097152" executionTimeout="3600"/>
  </system.web>
  <system.webServer>
    <httpModules> 
      <remove name="DomainServiceModule" />
    </httpModules>    
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler"
        preCondition="integratedMode,runtimeVersionv4.0"/>
    </handlers>
    <modules>
      <remove name="RemoveSecurityRelatedHeadersHttpModule"/>
    </modules>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="2147483648" />
      </requestFiltering>
    </security>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
